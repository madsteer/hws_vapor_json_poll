#set("body") {
    <p>JSON Polling is a leading polling service</p>
    <br/>
    <p>Choose a Poll to view:</p>
    <ul>
    #for(poll in allPolls) {
        <li><a href="/poll/#(poll.id)">#Capitalize(poll.title)</a></li>
    }
    </ul>
}

#if(poll) {
#set("title") {#capitalize(title)}
#set("h1") {#capitalize(title)}
#set("body") {
<p>
}
}

#set("title") {Welcome}
#embed("master")
